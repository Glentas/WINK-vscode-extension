{

    "Main Identifier": {
        "prefix": "main idtf",
        "body": [
            "=> nrel_main_idtf:",
            "\t[$1]",
            "\t(* <- lang_$2;; *);$0"
        ]
    },

    "Const Norole Relation (Arc)": {
        "prefix": "nrel_arc",
        "body": [
            "=> ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary norole relation (arc)"
    },

    "Var Norole Relation (Arc)": {
        "prefix": "_nrel_arc",
        "body": [
            "_=> ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary variable norole relation (arc)"
    },

    "Const Norole Relation-back": {
        "prefix": "nrel_arc-b",
        "body": [
            "<= ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary norole relation reversed"
    },

    "Var Norole Relation-back": {
        "prefix": "_nrel_arc-b",
        "body": [
            "_<= ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary variable norole relation reversed"
    },

    "Const Norole Relation (Edge)": {
        "prefix": "nrel_edge",
        "body": [
            "<=> ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary norole relation (edge)"
    },

    "Var Norole Relation (Edge)": {
        "prefix": "_nrel_edge",
        "body": [
            "<=> ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary variable norole relation (edge)"
    },

    "Role Relation": {
        "prefix": "rrel",
        "body": [
            "-> ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary role relation"
    },

    "Var Role Relation": {
        "prefix": "_rrel",
        "body": [
            "_-> ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary variable role relation"
    },

    "Role Relation-back": {
        "prefix": "rrel-b",
        "body": [
            "<- ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary role relation reversed"
    },

    "Var Role Relation-back": {
        "prefix": "_rrel-b",
        "body": [
            "<- ${1:relation}:",
            "\t${2:subject};$0"
        ],
        "description": "Binary variable role relation reversed"
    },

    "Set": {
        "prefix": "set",
        "body": [
            "{",
            "\t${1:element};",
            "\t${2:element}",
            "};$0"
        ]
    },

    "Tuple": {
        "prefix": "tuple",
        "body": [
            "...",
            "\t(*",
            "\t\t${1:<- node_1};;",
            "\t\t${2:-> node_2};;",
            "\t*);$0"
        ]
    },

    "Structure": {
        "prefix": "struct",
        "body": [
            "= [*",
            "\t{1:subject_1} => ${1:relation}:",
            "\t\t${2:subject};;$0",
            "*];$0"
        ]
    }
}