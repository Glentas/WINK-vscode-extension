{

    "Main Identifier": {
        "prefix": "main idtf",
        "body": [
            "=> nrel_main_idtf:",
            "\t[$1]",
            "\t(* <- lang_$2;; *);;$0"
        ]
    },

    "Const Non role Relation (Arc)": {
        "prefix": "nrel_const",
        "body": [
            "=> ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary non role relation (arc)"
    },

    "Var Non Role Relation (Arc)": {
        "prefix": "nrel_var",
        "body": [
            "_=> ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary variable non role relation (arc)"
    },

    "Const Non Role Relation-back": {
        "prefix": "nrel_const_backwards",
        "body": [
            "<= ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary non role relation reversed"
    },

    "Var Non Role Relation-back": {
        "prefix": "nrel_var_backwards",
        "body": [
            "<=_ ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary variable non role relation reversed"
    },

    "Const Non Role Relation (Edge)": {
        "prefix": "nrel_const_binary",
        "body": [
            "<=> ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary non role relation (edge)"
    },

    "Var Non Role Relation (Edge)": {
        "prefix": "nrel_var_binary",
        "body": [
            "_<=> ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary variable non role relation (edge)"
    },

    "Role Relation": {
        "prefix": "rrel_const",
        "body": [
            "-> ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary role relation"
    },

    "Var Role Relation": {
        "prefix": "rrel_var",
        "body": [
            "_-> ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary variable role relation"
    },

    "Role Relation-back": {
        "prefix": "rrel_const_backwards",
        "body": [
            "<- ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary role relation reversed"
    },

    "Var Role Relation-back": {
        "prefix": "rrel_var_backwards",
        "body": [
            "<-_ ${1:relation}:",
            "\t${2:subject};;$0"
        ],
        "description": "Binary variable role relation reversed"
    },

    "Non Oriented Set": {
        "prefix": ["non_oriented_set"],
        "body": [
            "{",
            "\t${1:element1};",
            "\t${2:attr2}: ${3:element2}",
            "};;$0"
        ],
        "description": "Non Oriented Set"
    },

    "Oriented Set": {
        "prefix": ["oriented_set"],
        "body": [
            "<",
            "\t${1:element1};",
            "\t${2:attr2}: ${3:element2}",
            ">;;$0"
        ],
        "description": "Oriented Set"
    },

    "Subitem": {
        "prefix": ["subitem"],
        "body": [
            "(*",
            "\t${1:-> element1};;",
            "\t${2:-> element2};;",
            "*);;$0"
        ],
        "description": "Subitem"
    },

    "Structure": {
        "prefix": ["structure"],
        "body": [
            "[*",
            "\t${1:element1} -> ${2:element2};;",
            "*];;$0"
        ],
        "description": "Structure"
    },

    "Number": {
        "prefix": ["number"],
        "body": [
            "[^\"${1:float}: $(2:52.42)\"];;$0"
        ],
        "description": "Number representation (sc link content)"
    },

    "String": {
        "prefix": ["string (strok)"],
        "body": [
            "[${1:my_string}];;$0"
        ],
        "description": "String representation (sc link content)"
    }
}